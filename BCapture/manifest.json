{
   "manifest_version": 2,
   "name": "Bearer Capture",
   "version": "1.0",
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "padootv.png"
   },
   "permissions": [
      "activeTab",
      "webRequest",
      "webRequestBlocking",
      "*://*/*",
      "tabs",
      "clipboardWrite",
      "storage"
   ]
}
